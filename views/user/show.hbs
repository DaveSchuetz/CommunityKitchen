<section>
  <h2>Welcome Back {{user.local.name}}</h2>
  <img src="../images/chef.jpg" alt ="Chef Image">
  <h3>Cookbooks</h3>
  {{#if user.cookbooks}}
    <div class="cookbooks">
      {{#each user.cookbooks}}
         {{> 'cookbook'}}
      {{/each}}
    </div>
  {{else}}
    <p>No Cookbooks yet</p>
  {{/if}}
  <h3>Recipes</h3>
  {{#if user.recipes}}
    <div class="recipes">
      {{#each user.recipes}}
         {{> 'recipe'}}
      {{/each}}
    </div>
  {{else}}
    <p>No Recipes yet</p>
  {{/if}}
  
</section>